# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                             videostreaming.controllers.Application.index
GET        /stream                       videostreaming.controllers.Application.stream

GET        /login                        videostreaming.controllers.Application.login
GET        /signup                       videostreaming.controllers.Application.signup

# Prefix must match `play.assets.urlPrefix`
GET        /assets/*file                 controllers.Assets.at(file)
GET        /versionedAssets/*file        controllers.Assets.versioned(path="/public", file: Asset)
